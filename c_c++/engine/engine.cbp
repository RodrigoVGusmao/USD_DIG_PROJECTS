<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="engine" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/engine" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add directory="include" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/engine" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-static" />
			<Add directory="$(#sdl2.include)" />
			<Add directory="$(#sdl2_image.INCLUDE)" />
			<Add directory="$(#boost.BASE)" />
		</Compiler>
		<Linker>
			<Add library="mingw32" />
			<Add library="SDL2main" />
			<Add library="SDL2.dll" />
			<Add library="user32" />
			<Add library="gdi32" />
			<Add library="winmm" />
			<Add library="dxguid" />
			<Add library="SDL2_image" />
			<Add directory="$(#sdl2.lib)" />
			<Add directory="$(#sdl2_image.LIB)" />
		</Linker>
		<ExtraCommands>
			<Add after="XCOPY $(#sdl2)\bin\*.dll $(TARGET_OUTPUT_DIR) /D /Y" />
			<Add after="XCOPY $(#sdl2_image.BIN)\*.dll $(TARGET_OUTPUT_DIR) /D /Y" />
		</ExtraCommands>
		<Unit filename="include/world.h" />
		<Unit filename="main.cpp" />
		<Unit filename="spriteSheetNumbers.png" />
		<Unit filename="spriteSheetTransparency.png" />
		<Unit filename="src/world.cpp" />
		<Unit filename="testPanelDuh.png" />
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
